<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
  
 <hibernate-mapping>  
  <class name="hibernate.Project" table="projects">  
    <id name="id">  
     <generator class="assigned"></generator>  
    </id>
            
    <property name="projectName"></property>
    <property name="clientName"></property>
    <property name="characterName"></property>
    <property name="artStyle"></property>
    <property name="specifications"></property>
    <property name="personCount"></property>
    <property name="price"></property>
    <property name="status"></property>
            
  </class>  
            
 </hibernate-mapping>  